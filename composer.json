{
    "name": "aimatchfun/laravel-ai",
    "description": "Laravel AI integration for multiple providers",
    "keywords": ["ai", "laravel", "provider", "openai", "anthropic"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AIMatchFun",
            "email": "diego@aimatch.fun"
        }
    ],
    "require": {
        "php": "^8.2|^8.3|^8.4",
        "illuminate/support": "^11.0|^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^7.0|^8.0",
        "phpunit/phpunit": "^9.0|^10.0"
    },
    "autoload": {
        "psr-4": {
            "AIMatchFun\\LaravelAI\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AIMatchFun\\LaravelAI\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "AIMatchFun\\LaravelAI\\Providers\\AIServiceProvider"
            ],
            "aliases": {
                "AI": "AIMatchFun\\LaravelAI\\Facades\\AI"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}